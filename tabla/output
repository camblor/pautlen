segment .data 
	err_div0 db "Error al dividir entre 0"
	err_index db "Indice de vector fuera de rango"
segment .bss 
	__esp resd 1
	_x1 resd 1
	_x2 resd 1
	_res resd 1
	_y1 resd 1
segment .text
	global main
	extern scan_int, print_int, scan_float, scan_boolean, print_boolean
	extern print_endofline, print_blank, print_string
	extern alfa_malloc, alfa_free, ld_float
main:
	mov dword [__esp] , esp
	push dword _x1
	call scan_int
	add esp, 4
	push dword _x2
	call scan_int
	add esp, 4
;D:	/
	push dword _x1
	push dword 5
	pop dword ebx
	pop dword eax
	mov dword eax, [eax]
	mov dword edx, 0
	cmp ebx, edx
	je err_div0
	idiv ebx
	push dword eax
	pop dword eax
	mov [_res], eax 
;D:	==
	push dword _x1
	push dword _x2
	pop dword edx
	pop dword eax
	mov dword edx, [edx]
	mov dword eax, [eax]
	cmp eax, edx
	je igual1
	push dword 0
	jmp fin_igual1
igual1:
	push dword 1
fin_igual1:
	pop dword eax
	mov [_y1], eax 
	push dword _y1
	pop dword eax
	mov eax, [eax]
	push dword eax
	call print_boolean
	add esp, 4
	call print_endofline
	jmp fin
fin_indice_fuera_rango:
	push dword err_index
	call print_string
	add esp, 4
	call print_endofline
	jmp fin
division_cero:
	push dword err_div0
	call print_string
	add esp, 4
	call print_endofline
fin:
	mov dword esp, [__esp]
	ret
